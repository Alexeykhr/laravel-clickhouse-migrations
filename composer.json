{
    "name": "alexeykhr/laravel-clickhouse-migrations",
    "description": "Clickhouse migrations for Laravel",
    "keywords": ["clickhouse", "migrations", "laravel", "package", "library"],
    "license": "MIT",
    "homepage": "https://github.com/alexeykhr/laravel-clickhouse-migrations",
    "authors": [
        {
            "name": "Alexey Khrushch",
            "email": "alexeykhr@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.21",
        "illuminate/contracts": "^7.21",
        "illuminate/filesystem": "^7.21",
        "illuminate/support": "^7.21",
        "smi2/phpclickhouse": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Alexeykhr\\ClickhouseMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alexeykhr\\ClickhouseMigrations\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Alexeykhr\\ClickhouseMigrations\\Providers\\MigrationProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
